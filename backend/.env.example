# Server Configuration
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/undertheradar_vpn
DB_SSL=false

# Authentication
JWT_SECRET=your-super-secret-jwt-key
JWT_EXPIRE=7d
ADMIN_JWT_EXPIRE=1d

# Redis
REDIS_URL=redis://localhost:6379

# Stripe
STRIPE_SECRET_KEY=sk_test_your_stripe_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# AWS
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1

# Email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# VPN Server Management
VPN_API_KEY=your-vpn-management-api-key
WIREGUARD_CONFIG_PATH=/etc/wireguard

# Domain
DOMAIN=undertheradar.work
API_URL=https://api.undertheradar.work
DASHBOARD_URL=https://dashboard.undertheradar.work